    "import re\n",
    "import unicodedata\n",
    "from langchain.schema import Document\n",
    "\n",
    "CTRL_CHARS = (\n",
    "    r\"[\\u0000-\\u001F\\u007F-\\u009F]\"           # C0/C1 control\n",
    "    r\"|[\\u200B-\\u200F]\"                        # zero-width/ZWNJ/ZWNJ/RTL marks\n",
    "    r\"|[\\u202A-\\u202E]\"                        # bidi controls\n",
    "    r\"|\\u2060|\\u2066|\\u2067|\\u2068|\\u2069\"     # word joiner & bidi isolates\n",
    "    r\"|\\ufeff\"                                 # BOM\n",
    ")\n",
    "\n",
    "def clean_text(text: str) -> str:\n",
    "    # 0) Unicode 정규화 (호환형) — 전각/라틴 변형 등 정돈\n",
    "    t = unicodedata.normalize(\"NFKC\", text)\n",
    "\n",
    "    # 1) 제어문자/제로폭 문자 제거\n",
    "    t = re.sub(CTRL_CHARS, \"\", t)\n",
    "\n",
    "    # 2) PDF 추출 흔적 정리\n",
    "    #   - 페이지 번호 라인: \"- 4 -\" 같은 것 제거\n",
    "    t = re.sub(r\"(?m)^\\s*-\\s*\\d+\\s*-\\s*$\", \"\", t)\n",
    "    #   - 머리말/꼬리말 패턴이 일정하면 여기서 추가로 제거 규칙 더 넣기\n",
    "\n",
    "    # 3) 줄바꿈/하이픈 줄바꿈 처리\n",
    "    #   - 줄 끝 하이픈으로 단어가 잘린 경우 붙이기: \"개발-\\n목표\" → \"개발목표\"\n",
    "    t = re.sub(r\"(\\w)-\\n(\\w)\", r\"\\1\\2\", t)\n",
    "    #   - 문장 중간에 부자연스런 단일 개행을 공백으로 치환\n",
    "    #     (단, 문장부호로 끝나는 줄은 단락 유지)\n",
    "    t = re.sub(r\"(?<![.!?。…])\\n(?!\\n)\", \" \", t)\n",
    "\n",
    "    # 4) 리스트 불릿/구분자 정리 (원한다면 통일)\n",
    "    #   • ● ▪ ▸ ▶ ► ▷ 등을 \"- \"로 통합하거나 유지\n",
    "    t = re.sub(r\"(?m)^[\\s•●▪▸▶►▷]+\\s*\", \"- \", t)\n",
    "\n",
    "    # 5) 허용 문자만 남기고 나머지 제거(옵션)\n",
    "    pattern = r'[^\\x20-\\x7E가-힣ㄱ-ㅎㅏ-ㅣ\\s·‧\\-–—:;.,!?()\\{\\}\\[\\]“”\"%&\\'/⋯▸▶►▷•●▪]'\n",
    "    t = re.sub(pattern, \"\", t)\n",
    "    # 6) 공백 정리\n",
    "    t = re.sub(r\"[ \\t]+\", \" \", t)\n",
    "    t = re.sub(r\"\\n{3,}\", \"\\n\\n\", t).strip()\n",
    "\n",
    "    return t\n",
    "\n",
    "clean_docs = [\n",
    "    Document(\n",
    "        page_content=clean_text(doc.page_content),\n",
    "        metadata=doc.metadata\n",
    "    )\n",
    "    for doc in pdf_docs\n",
    "]\n",
    "\n",
    "print(\"총 페이지 수:\", len(clean_docs))\n",
    "print(\"1페이지:\", clean_docs[0].page_content[:500])\n"
   ]
  },
